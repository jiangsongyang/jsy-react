{
  "name": "jsy-react",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "16.13.0",
    "pnpm": "7.1.7"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "install:commit-msg-lint": "npx husky add .husky/commit-msg \"node scripts/commit-message-lint/commit-msg.mjs $1\"",
    "install:pre-commit-lint": "npx husky add .husky/pre-commit \"npx lint-staged $1\"",
    "<!-- dev -->": "",
    "dev:demo": "vite serve demos/test-useContext --config ./scripts/vite/vite.config.js --force",
    "<!-- build -->": "",
    "build:dev": "rimraf dist && rollup --bundleConfigAsCjs --config scripts/rollup/dev.config.js",
    "<!-- test -->": "",
    "test": "jest --config scripts/jest/jest.config.js",
    "<!-- lint -->": "",
    "lint:eslint": "npx eslint \"./packages/**\" --fix",
    "lint:prettier": "npx prettier \"./packages\" --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm precommit",
      "commit-msg": "node scripts/commit-msg.mjs"
    }
  },
  "lint-staged": {
    "./packages/**/*.{ts,tsx}": [
      "pnpm lint:eslint",
      "pnpm lint:prettier",
      "git add ."
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jiangsongyang/jsy-react.git"
  },
  "keywords": [],
  "author": "jiangsongyang",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/eslint": "^8.4.5",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.7.6",
    "@types/postcss-preset-env": "^7.7.0",
    "@types/react": "^18.0.17",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.0.6",
    "@types/scheduler": "^0.16.3",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-react": "^3.1.0",
    "chalk": "^5.0.1",
    "eslint": "^8.22.0",
    "eslint-config-jsy": "^1.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "jest": "^29.4.3",
    "jest-config": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "prettier": "^2.7.1",
    "rimraf": "^4.0.7",
    "rollup": "^3.10.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "tslib": "^2.4.1",
    "typescript": "^4.7.4",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "scheduler": "^0.23.0"
  }
}
